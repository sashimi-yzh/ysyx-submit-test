Subject: [PATCH] disable output in uart_tfifo.v

---
 perip/uart16550/rtl/uart_tfifo.v | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/perip/uart16550/rtl/uart_tfifo.v b/perip/uart16550/rtl/uart_tfifo.v
index 713291164..b788bc2d0 100644
--- a/perip/uart16550/rtl/uart_tfifo.v
+++ b/perip/uart16550/rtl/uart_tfifo.v
@@ -211,7 +211,7 @@ begin
             begin
                 top   <= #1 top_plus_1;
                 count <= #1 count + 1'b1;
-                $write("%c", data_in);
+                //$write("%c", data_in);
             end
         2'b01 : if(count>0)
             begin
@@ -221,7 +221,7 @@ begin
         2'b11 : begin
                 bottom <= #1 bottom + 1'b1;
                 top    <= #1 top_plus_1;
-                $write("%c", data_in);
+                //$write("%c", data_in);
                 end
         default: ;
         endcase
