# CONFIG_ISA_mips32 is not set
CONFIG_ISA_riscv=y
CONFIG_ISA="riscv32"

#
# ISA-dependent Options for riscv
#
# CONFIG_RV64 is not set
CONFIG_RVE=y
# end of ISA-dependent Options for riscv

#
# Configuration for NPC
#
# CONFIG_USE_ICACHE is not set
# CONFIG_USE_BPU is not set
# CONFIG_PERF_CNT is not set
# CONFIG_USE_REG0 is not set
# end of Configuration for NPC

# CONFIG_CC_O0 is not set
# CONFIG_CC_O1 is not set
CONFIG_CC_O2=y
# CONFIG_CC_O3 is not set
CONFIG_CC_OPT="-O2"

#
# Simulation Configuration
#
# CONFIG_WAVE_DUMP is not set
CONFIG_WAVE_FILE_TYPE="none"
# end of Simulation Configuration

#
# Testing and Debugging
#
# CONFIG_PC_TRACE is not set
# CONFIG_TRACE is not set
# CONFIG_DIFFTEST is not set
CONFIG_DIFFTEST_REF_PATH="none"
CONFIG_DIFFTEST_REF_NAME="none"
# end of Testing and Debugging

#
# Memory Configuration(Won't be Uesd: Don't Touch)
#
CONFIG_MBASE=0x80000000
CONFIG_MSIZE=0xf000000
CONFIG_PC_RESET_OFFSET=0
# CONFIG_PMEM_MALLOC is not set
CONFIG_PMEM_GARRAY=y
# end of Memory Configuration(Won't be Uesd: Don't Touch)

CONFIG_DEVICE=y
CONFIG_HAS_SERIAL=y
CONFIG_SERIAL_MMIO=0xa00003f8
# CONFIG_SERIAL_INPUT_FIFO is not set
CONFIG_HAS_TIMER=y
CONFIG_RTC_MMIO=0xa0000048
# CONFIG_HAS_KEYBOARD is not set
# CONFIG_HAS_VGA is not set
# CONFIG_HAS_AUDIO is not set
# CONFIG_HAS_DISK is not set
# CONFIG_HAS_SDCARD is not set
